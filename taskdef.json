{
    "executionRoleArn": "arn:aws:iam::331230890924:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sample-app",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "healthCheck": {
                "command": [
                  "CMD-SHELL",
                  "echo hello"
                ],
                "interval": 5,
                "timeout": 2,
                "retries": 3
            } 
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "adidas-test"
}
