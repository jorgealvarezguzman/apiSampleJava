{
    "executionRoleArn": "arn:aws:iam::331230890924:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "sample-app",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "healthCheck": {
              "retries": 10,
              "command": [
                  "CMD-SHELL",
                  "curl -f http://localhost:80/ || exit 1"
              ],
              "timeout": 120,
              "interval": 300,
              "startPeriod": null
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "adidas-test"
}
